# Aster Finance API 配置
BASE_URL=https://sapi.asterdex.com

# 账户1配置
ACCOUNT_1_NAME=主交易账户
ACCOUNT_1_API_KEY=your_api_key_1_here
ACCOUNT_1_SECRET_KEY=your_secret_key_1_here

# 账户2配置  
ACCOUNT_2_NAME=备用账户
ACCOUNT_2_API_KEY=your_api_key_2_here
ACCOUNT_2_SECRET_KEY=your_secret_key_2_here

# 账户3配置
ACCOUNT_3_NAME=量化账户
ACCOUNT_3_API_KEY=your_api_key_3_here
ACCOUNT_3_SECRET_KEY=your_secret_key_3_here

# 要统计的交易对 (用逗号分隔)
SYMBOLS=BTCUSDT,ETHUSDT,BNBUSDT,ADAUSDT,SOLUSDT,XRPUSDT,DOTUSDT,LINKUSDT,LTCUSDT,BCHUSDT

# 统计时间范围 (天)
STAT_DAYS=30

# 请求延迟 (秒，避免频繁请求)
REQUEST_DELAY=0.2